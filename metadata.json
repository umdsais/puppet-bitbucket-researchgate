{
  "name": "thewired-bitbucket",
  "version": "2.3.1",
  "author": "thewired",
  "summary": "Install Atlassian Bitbucket Server",
  "license": "MIT",
  "source": "https://github.com/danofthewired/puppet-bitbucket.git",
  "project_page": "https://github.com/danofthewired/puppet-bitbucket",
  "issues_url": "https://github.com/danofthewired/puppet-bitbucket/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.0.0 < 5.0.0"
    },
    {
      "name": "puppetlabs/inifile",
      "version_requirement": ">= 1.0.0 < 2.0.0"
    },
    {
      "name": "mkrakowitzer/deploy",
      "version_requirement": ">= 0.0.3 < 1.0.0"
    },
    {
      "name": "puppet/staging",
      "version_requirement": ">= 1.0.0 < 2.0.0"
    },
    {
      "name": "puppet/archive",
      "version_requirement": ">= 0.4.4 < 1.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7",
        "8",
	"9"
      ]
