    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "20.04",
        "22.04",
        "24.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "11",
        "12"
      ]
    }

  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=7.0.0 < 9.0.0"
    }
  ],
  "description": "Module to install Atlassian Bitbucket Server/Data Center. On-premises source code management for Git that's secure, fast, and enterprise grade.",
  "pdk-version": "3.0.1",
  "template-url": "pdk-default#3.0.1",
  "template-ref": "tags/3.0.1-0-gd13288a"
}
